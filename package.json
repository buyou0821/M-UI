{
  "name": "tyche-ui",
  "version": "0.1.2",
  "description": "A UI library for React",
  "main": "lib/index.js",
  "module": "es/index.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test jest --config=jest.config.js --runInBand --no-cache",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config scripts/config/webpack.site.dev.ts",
    "docs": "cross-env NODE_ENV=production webpack --config scripts/config/webpack.site.docs.ts",
    "dist": "cross-env NODE_ENV=production webpack --config scripts/config/webpack.dist.ts",
    "svg": "ts-node scripts/template/svg.ts && yarn lint:fix",
    "watch:svg": "nodemon --exec ts-node -- scripts/template/svg.ts",
    "compile": "tyche-tools run compile",
    "prettier": "prettier --write --config .prettierrc 'components/**/*.{ts,tsx,js,jsx}'",
    "lint-fix:es": "eslint --fix --ext .js,.tsx components && npm run lint:style",
    "lint:ts": "tslint --project tsconfig.json 'components/**/*.{ts,tsx}'",
    "lint:style": "prettier-stylelint --write -q 'components/**/*.{css,scss,less}'",
    "lint:fix": "npm run prettier && tslint --fix --project tsconfig.json 'components/**/*.{ts,tsx}' && npm run lint:style",
    "ci": "cross-env NODE_ENV=test JEST_JUNIT_OUTPUT=./test-results/jest/results.xml jest --no-cache --config=.jest.ci.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/buyou0821/tyche-ui.git"
  },
  "keywords": [
    "tyche",
    "react",
    "typescript",
    "component",
    "components",
    "ui",
    "frontend"
  ],
  "author": "shenming",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/buyou0821/tyche-ui/issues"
  },
  "homepage": "https://github.com/buyou0821/tyche-ui#readme",
  "devDependencies": {
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-react": "^7.0.0",
    "@testing-library/react": "^8.0.6",
    "@types/classnames": "^2.2.7",
    "@types/html-webpack-plugin": "^3.2.1",
    "@types/jest": "^24.0.11",
    "@types/mini-css-extract-plugin": "^0.2.1",
    "@types/react": "^16.8.8",
    "@types/react-dom": "^16.8.3",
    "@types/react-router-dom": "^4.3.1",
    "@types/react-test-renderer": "^16.8.1",
    "@types/react-transition-group": "^2.8.0",
    "@types/svgo": "^1.2.0",
    "@types/webpack": "^4.4.35",
    "@types/webpack-dev-server": "^3.1.7",
    "@types/webpack-merge": "^4.1.5",
    "add": "^2.0.6",
    "awesome-typescript-loader": "^5.2.1",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.5.0",
    "babel-loader": "^8.0.6",
    "classnames": "^2.2.6",
    "clean-webpack-plugin": "^2.0.1",
    "codemirror": "^5.47.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.15.3",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-markdown": "^1.0.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.5.0",
    "jest-junit": "^6.3.0",
    "markdown-loader": "2.0.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.16.4",
    "prettier-stylelint": "^0.4.2",
    "react-markdown-reader": "^1.1.0",
    "react-router-dom": "^5.0.0",
    "react-test-renderer": "^16.8.5",
    "rucksack-css": "^1.0.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "stylelint-config-prettier": "^5.0.0",
    "stylelint-config-rational-order": "^0.0.4",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-declaration-block-no-ignored-properties": "^1.1.0",
    "stylelint-scss": "^3.5.4",
    "svg-sprite-loader": "^4.1.3",
    "svgo": "^1.2.2",
    "ts-jest": "^24.0.1",
    "ts-node": "^8.3.0",
    "tslint": "^5.14.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-react": "^3.6.0",
    "tyche-tools": "^0.0.3",
    "typescript": "^3.4.5",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1",
    "webpack-merge": "^4.2.1",
    "yarn": "^1.13.0"
  },
  "dependencies": {
    "@types/autosize": "^3.0.6",
    "@types/lodash-es": "^4.17.3",
    "autosize": "^4.0.2",
    "clsx": "^1.0.4",
    "enquire.js": "^2.1.6",
    "lodash-es": "^4.17.11",
    "react": "^16.8.5",
    "react-dom": "^16.8.5",
    "react-transition-group": "^2.9.0"
  },
  "files": [
    "dist",
    "lib",
    "es"
  ],
  "activationEvents": [
    "workspaceContains:node_modules/.bin/jest",
    "workspaceContains:node_modules/react-scripts/node_modules/.bin/jest",
    "workspaceContains:yarn test",
    "onCommand:io.orta.jest.start"
  ]
}
